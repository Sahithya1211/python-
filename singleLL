class Node:
    def __init__(self,val):
        self.data=val
        self.next=None
def CreateLinkedlist(arr):
    head=None
    for val in arr:
        if(head == None):
            head = None(val)
            temp = head
        else:
            temp.next = Node(val)
            temp = temp.next
arr=list(map(int,input().split()))
CreateLinkedlist(arr)
